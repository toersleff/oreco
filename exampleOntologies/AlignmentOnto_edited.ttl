@prefix : <https://www.hsu-ifa.de/ontologies/PeakEvaluation01AlignmentOntology#> .
@prefix mod: <https://w3id.org/mod#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix s4ener: <https://saref.etsi.org/saref4ener/> .
@prefix topo: <https://www.hsu-ifa.de/ontologies/LVGridTopology#> .

: a owl:Ontology ;
    owl:imports s4ener:,
        <https://www.hsu-ifa.de/ontologies/LVGridTopology/>,
        <https://www.hsu-ifa.de/ontologies/SAREF4ENERExtension/> ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

:hasLocation a owl:ObjectProperty ;
    rdfs:label "has location" ;
    rdfs:domain s4ener:FlexOffer,
        s4ener:FlexRequest ;
    rdfs:range topo:Node .

