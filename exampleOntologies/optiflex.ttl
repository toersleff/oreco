 @prefix : <http://www.hsu-ifa.de/ontologies/OptiFlex#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

: a owl:Ontology ;
    dc:description "Ontology code created by Chowlk" .

:activationGradientinKilowattPerSecond a owl:DatatypeProperty ;
    rdfs:label "activation gradientin kilowatt per second" ;
    rdfs:domain :FlexibleLoad ;
    rdfs:range xsd:double .

:costInEuro a owl:DatatypeProperty ;
    rdfs:label "cost in euro" ;
    rdfs:domain :FlexibleLoad ;
    rdfs:range xsd:double .

:earliestStart a owl:DatatypeProperty ;
    rdfs:label "earliest start" ;
    rdfs:domain :FlexibleLoad ;
    rdfs:range xsd:dateTime .

:hasTemporalType a owl:ObjectProperty ;
    rdfs:label "has temporal type" ;
    rdfs:domain :FlexibleLoadDependency ;
    rdfs:range :TemporalType .

:holdingDurationInSeconds a owl:DatatypeProperty ;
    rdfs:label "holding duration in seconds" ;
    rdfs:domain :FlexibleLoad ;
    rdfs:range xsd:int .

:isFlexibleLoadOf a owl:ObjectProperty ;
    rdfs:label "is flexible load of" ;
    owl:inverseOf :hasFlexibleLoad .

:isTargetFlexibleLoadOf a owl:ObjectProperty ;
    rdfs:label "is target flexible load of" ;
    owl:inverseOf :hasTargetFlexibleLoad .

:isTriggerFlexibleLoadOf a owl:ObjectProperty ;
    rdfs:label "is trigger flexible load of" ;
    owl:inverseOf :hasTriggerFlexibleLoad .

:latestStart a owl:DatatypeProperty ;
    rdfs:label "latest start" ;
    rdfs:domain :FlexibleLoad ;
    rdfs:range xsd:dateTime .

:powerStateInKilowatts a owl:DatatypeProperty ;
    rdfs:label "power state in kilowatts" ;
    rdfs:domain :FlexibleLoad ;
    rdfs:range xsd:double .

:temporalGapInSeconds a owl:DatatypeProperty ;
    rdfs:label "temporal gap in seconds" ;
    rdfs:domain :FlexibleLoadDependency ;
    rdfs:range xsd:int .

:Process a owl:Class ;
    rdfs:label "Process" .

:TemporalType a owl:Class ;
    rdfs:label "Temporal Type" ;
    owl:equivalentClass [ a owl:Class ;
            owl:oneOf ( :end-start :start-end :end-end :start-start ) ] .

:end-end a owl:NamedIndividual .

:end-start a owl:NamedIndividual .

:hasFlexibleLoad a owl:ObjectProperty ;
    rdfs:label "has flexible load" ;
    rdfs:domain :Process ;
    rdfs:range :FlexibleLoad .

:hasTargetFlexibleLoad a owl:ObjectProperty ;
    rdfs:label "has target flexible load" ;
    rdfs:domain :FlexibleLoadDependency .

:hasTriggerFlexibleLoad a owl:ObjectProperty ;
    rdfs:label "has trigger flexible load" ;
    rdfs:domain :FlexibleLoadDependency .

:start-end a owl:NamedIndividual .

:start-start a owl:NamedIndividual .

:FlexibleLoadDependency a owl:Class ;
    rdfs:label "Flexible Load Dependency" .

:FlexibleLoad a owl:Class ;
    rdfs:label "Flexible Load" .

#################################################################
#    Individuals
#################################################################

###  http://www.hsu-ifa.de/ontologies/OptiFlex#end-end
:end-end rdf:type owl:NamedIndividual ,
                  :TemporalType .


###  http://www.hsu-ifa.de/ontologies/OptiFlex#end-start
:end-start rdf:type owl:NamedIndividual ,
                    :TemporalType .


###  http://www.hsu-ifa.de/ontologies/OptiFlex#flexibleLoad1
:flexibleLoad1 rdf:type owl:NamedIndividual ,
                        :FlexibleLoad ;
               :activationGradientInKilowattPerSecond "0.1"^^xsd:double ;
               :costInEuro "2.7"^^xsd:double ;
               :earliestStart "2022-03-18T19:20:00"^^xsd:dateTime ;
               :holdingDurationInSeconds "600"^^xsd:int ;
               :latestStart "2022-03-18T22:00:00"^^xsd:dateTime ;
               :powerStateInKilowatts "7.0"^^xsd:double .


###  http://www.hsu-ifa.de/ontologies/OptiFlex#flexibleLoad2
:flexibleLoad2 rdf:type owl:NamedIndividual ,
                        :FlexibleLoad ;
               :activationGradientInKilowattPerSecond "0.1"^^xsd:double ;
               :costInEuro "3.1"^^xsd:double ;
               :earliestStart "2022-03-18T20:10:00"^^xsd:dateTime ;
               :holdingDurationInSeconds "1200"^^xsd:int ;
               :latestStart "2022-03-18T23:30:00"^^xsd:dateTime ;
               :powerStateInKilowatts "3.0"^^xsd:double .


###  http://www.hsu-ifa.de/ontologies/OptiFlex#flexibleLoadDependency1
:flexibleLoadDependency1 rdf:type owl:NamedIndividual ,
                                  :FlexibleLoadDependency ;
                         :hasTargetFlexibleLoad :flexibleLoad2 ;
                         :hasTemporalType :start-start ;
                         :hasTriggerFlexibleLoad :flexibleLoad1 ;
                         :temporalGapInSeconds "400"^^xsd:int .


###  http://www.hsu-ifa.de/ontologies/OptiFlex#process1
:process1 rdf:type owl:NamedIndividual ,
                   :Process ;
          :hasFlexibleLoad :flexibleLoad1 ,
                           :flexibleLoad2 .


###  http://www.hsu-ifa.de/ontologies/OptiFlex#start-end
:start-end rdf:type owl:NamedIndividual ,
                    :TemporalType .


###  http://www.hsu-ifa.de/ontologies/OptiFlex#start-start
:start-start rdf:type owl:NamedIndividual ,
                      :TemporalType .
